<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.aaa.qy111mp.dao.NotesDao">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.aaa.qy111mp.entity.Notes">
        <id column="nid" property="nid" />
        <result column="title" property="title" />
        <result column="start_time" property="startTime" />
        <result column="stop_time" property="stopTime" />
        <result column="ndesc" property="ndesc" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        nid, title, start_time AS startTime, stop_time AS stopTime, ndesc
    </sql>

</mapper>
